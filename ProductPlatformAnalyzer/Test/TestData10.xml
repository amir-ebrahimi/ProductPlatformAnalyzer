<?xml version="1.0"?>
<!-- Testcase break test counter example
		Test can not be preformed since sensors can't be removed. Removing of sensors has the precondition that run-test should be finished (works when condition is on executing state). May choose either intenal or external sensors, they ahve the same setup and gice the same result.

		 Truck variant: axel1
		 Test variant:  break-test
		 Sendors: External, Internal
		 Operations: run-break_test, put on sensor, remove sensor, turn on sensor, turn off sensors

		 axel1 and test_break -> run_break_test
		 axel2 and test_break -> run_break_test

		 Condition: 

	Mods on output:
		Choose exactly one with 1 variant
		[one-axel and axel-test] => run test
			Variant operation mapping [V] -> Op
-->
<testData>
<operations>
	<operation>
		<operationName>run-break-test</operationName>
		<displayName>run break test</displayName>
		<operationPrecondition>
			<operationName>activate-sensors_F</operationName>
		</operationPrecondition>
		<operationPostcondition>
			<operationName>deactivate-sensors_F</operationName>
		</operationPostcondition>
	</operation>
	<operation>
		<operationName>activate-sensors</operationName>
		<displayName>activate sensors</displayName>
	</operation>
	<operation>
		<operationName>deactivate-sensors</operationName>
		<displayName>deactivate sensors</displayName>
		<operationPrecondition>
			<operationName>run-break-test_F</operationName>
		</operationPrecondition>
	</operation>
</operations>
<variants>
	<variant>
		<variantName>axel1</variantName>
		<variantIndex>1</variantIndex>
		<variantDisplayName>axel 1</variantDisplayName>
	</variant>
	<variant>
		<variantName>axel2</variantName>
		<variantIndex>2</variantIndex>
		<variantDisplayName>axel2</variantDisplayName>
	</variant>
	<variant>
		<variantName>internal-sensors</variantName>
		<variantIndex>3</variantIndex>
		<variantDisplayName>internal sensors</variantDisplayName>
	</variant>
	<variant>
		<variantName>external-sensors</variantName>
		<variantIndex>4</variantIndex>
		<variantDisplayName>external sensors</variantDisplayName>
	</variant>
	<variant>
		<variantName>break-test</variantName>
		<variantIndex>5</variantIndex>
		<variantDisplayName>break test</variantDisplayName>
	</variant>
  <variant>
    <variantName>VirtualV1</variantName>
    <variantIndex>6</variantIndex>
    <variantDisplayName>Virtual V1</variantDisplayName>
  </variant>
  <variant>
    <variantName>VirtualV2</variantName>
    <variantIndex>7</variantIndex>
    <variantDisplayName>Virtual V2</variantDisplayName>
  </variant>
</variants>
<variantGroups>
	<variantGroup>
		<variantGroupName>test</variantGroupName>
		<groupCardinality>choose at least one</groupCardinality>
		<variantRefs>
			<variantRef>break-test</variantRef>
		</variantRefs>
	</variantGroup>
	<variantGroup>
		<variantGroupName>sensors</variantGroupName>
		<groupCardinality>choose exactly one</groupCardinality>
		<variantRefs>
			<variantRef>external-sensors</variantRef>
			<variantRef>internal-sensors</variantRef>
		</variantRefs>
	</variantGroup>
	<variantGroup>
		<variantGroupName>axel</variantGroupName>
		<groupCardinality>choose at least one</groupCardinality>
		<variantRefs>
			<variantRef>axel1</variantRef>
		</variantRefs>
	</variantGroup>
  <variantGroup>
    <variantGroupName>Virtual-VG</variantGroupName>
    <groupCardinality>choose at least one</groupCardinality>
    <variantRefs>
      <variantRef>VirtualV1</variantRef>
      <variantRef>VirtualV2</variantRef>
    </variantRefs>
  </variantGroup>
</variantGroups>
<constraints>
  <constraint>
    <logic>or (and (and (and break-test axel1) internal-sensors) VirtualV1) (and (not (and (and break-test axel1) internal-sensors)) (not VirtualV1))</logic>
  </constraint>
  <constraint>
    <logic>or (and (and (and break-test axel1) external-sensors) VirtualV2) (and (not (and (and break-test axel1) external-sensors)) (not VirtualV2))</logic>
  </constraint>
</constraints>
<variantOperationMappings>
	<variantOperationMapping>
		<variantRefs>
	    	<variantRef>VirtualV1</variantRef>
	    </variantRefs>
		<operationRefs>
		   <operationRef>run-break-test</operationRef>
		   <operationRef>activate-sensors</operationRef>
		   <operationRef>deactivate-sensors</operationRef>
		</operationRefs>
	</variantOperationMapping>
	<variantOperationMapping>
		<variantRefs>
	    	<variantRef>VirtualV2</variantRef>
	    </variantRefs>
		<operationRefs>
		   <operationRef>run-break-test</operationRef>
		   <operationRef>activate-sensors</operationRef>
		   <operationRef>deactivate-sensors</operationRef>
		</operationRefs>
	</variantOperationMapping>
</variantOperationMappings>
</testData>